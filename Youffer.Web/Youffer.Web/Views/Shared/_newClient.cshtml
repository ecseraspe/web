@model Youffer.Web.Resources.ViewModel.WebClientWishListModel
@using Youffer.Web.Common.Extensions
@using Youffer.Web.Filter
@if (Model.SubInterest.IsAny())
{
    int subInterestCount = Model.SubInterest.Count();
    Random rnd = new Random();
    int interest = rnd.Next(0, subInterestCount);   
    <div class="client-container-new">
        <div class="lead-new block-client-new">
            <div class="client-name">
                @if (Model.Client.Age > 0)
                {
                    @(Model.Client.FirstName + ", " + Model.Client.Age)
                }
                else
                {
                    @Model.Client.FirstName
                }
            </div>
            <div class="price-tag-new">
                <em class="order-new">Order-</em>
                    <em>@YoufferSecurity.CurrentAmountWithCurrency</em>                
            </div>
            <div style="display: inherit;width: 100%;">
                @if (!string.IsNullOrWhiteSpace(Model.Client.ImageUrl))
                {
                    <div class="roundImages" style="position: relative; background: url(@Model.Client.ImageUrl) center no-repeat; background-size: cover;">
                        <div class="strip">
                            @if (Convert.ToInt16(Model.Ranking) == 5)
                            {
                                <img src="data:image/png;base64," />
                            }
                        </div>
                    </div><br />
                }
                else
                {
                    <div class="roundImages" style="position: relative; background: url(@Youffer.Web.Common.Dummy.DummyData.NoImage) center no-repeat; background-size: cover;">
                        <div class="strip">
                            @if (Convert.ToInt16(Model.Ranking) == 5)
                            {
                                <img src="data:image/png;base64," />
                            }
                        </div>
                    </div><br />
                }
                <div class="interest-in-new" title="@Model.SubInterest[interest]">
                    <h5>Needs:</h5>
                    <h4>@Model.SubInterest[interest]</h4>
                </div>
                <div class="button-container">
                    @if (Youffer.Web.Filter.YoufferSecurity.IsAuthenticated)
                    {
                        if (!String.IsNullOrEmpty(Model.PurchasedInterest))
                        {
                            <a style="cursor:pointer;" onclick="GetclientProfileAndReview('@Model.Client.ClientId', '@Model.PurchasedInterest',true);"> View Profile </a>
                        }
                        else
                        {
                            <a style="cursor:pointer;" onclick="GetclientProfileAndReview('@Model.Client.ClientId', '@Model.SubInterest[interest]');"> View Profile </a>
                        }
                        using (Html.BeginForm("Checkout", "Payment", FormMethod.Post))
                        {
                            <input type="submit" class="buy-button-new" style="margin-top:10px;" value="Buy (@YoufferSecurity.CurrentAmountWithCurrency)" /> <br />                           
                            @*<input type="submit" class="buy-button-new" style="margin-top:10px;" value="Buy ($5)" /> <br />*@
                            @Html.Hidden("clientInterest", Model.SubInterest[interest]);
                            @Html.Hidden("clientId", Model.WishListId);
                        }
                        }
                    else
                    {
                        <a style="cursor:pointer;" onclick="ShowRegisterForm(); return false;" href="#"> View Profile </a>
                        <input type="submit" class="buy-button-new" onclick="ShowRegisterForm(); return false;" style="margin-top:10px;" value="Buy (@YoufferSecurity.CurrentAmountWithCurrency)" /> <br />
                    }
                    <div class="clearfix-new"></div>
                </div>
                <hr class="client-box-hr">
                <div style="padding: 0 7px 7px 7px;">
                    @if (!string.IsNullOrWhiteSpace(Model.Client.Flag))
                    {<img class="flag-new" src="@Model.Client.Flag" />}
                    <div class="rating-new">
                        <span class="star-new">
                            @{Html.RenderAction("GetRating", "Common", new { rating = Model.Ranking, disabled = true, className = Guid.NewGuid(), title = "" });}
                        </span>
                    </div>
                    <div class="clearfix-new"></div>
                </div>
                <div class="clearfix-new"></div>
            </div>
            <span class="clearfix-new"></span>
        </div>
    </div>
}

