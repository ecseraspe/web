        <script>
            $(document).ready(function () {
                $('#leftPanel a').removeClass("active");
                if (window.location.href.indexOf('/MyClients') > -1) {
                    $('#myClients').addClass("active");
                }
                else if (window.location.href.indexOf('/Dashboard') > -1) {
                    $('#home').addClass("active");
                }
                else if (window.location.href.indexOf('/ReviewClient') > -1) {
                    $('#reviewClients').addClass("active");
                }
                else if (window.location.href.indexOf('/Invoices') > -1) {
                    $('#invoices').addClass("active");
                }
                else if (window.location.href.indexOf('/Search') > -1) {
                    $('#search').addClass("active");
                }
                else if (window.location.href.indexOf('/AllMessages') > -1) {
                    $('#messages').addClass("active");
                }
                else if (window.location.href.indexOf('/Settings') > -1) {
                    $('#settings').addClass("active");
                }
                else if (window.location.href.indexOf('/Statistics') > -1) {
                    $('#statistics').addClass("active");
                }
                else if (window.location.href.indexOf('/Contact') > -1) {
                    $('#contact').addClass("active");
                }
            })
        </script>

