                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="@Url.Action("WhatIsYouffer", "Home")">What is Youffer?</a>
                        </li>

                        <li>
                            <a href="@Url.Action("HowItWorks", "Home")">How It Works?</a>
                        </li>

                        <li>
                            <a target="_blank" href="/Download" style="color: #1699cd; font-weight: 400;">Download App</a>
                        </li>

                        <li class="login-popup">
                            <a class="login-btn-new" style="cursor:pointer" onclick="ShowloginForm();">Login</a>
                        </li>

                        <li class="register-popup">
                            <a class="signup-btn" href="#" style="cursor:pointer" onclick="ShowRegisterForm();">Register</a>
                        </li>
                    </ul>
                    @*<div id="_register"></div>
                        <div id="_login"></div>*@
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container -->
        </nav>
        <div class="popup-register" id="_register">
            @{ Html.RenderAction("Register", "Account");}
        </div>
        <div class="popup-login" id="_login">
            @{ Html.RenderAction("Login", "Account");}
        </div>

        <div class="popup-ForgotPassword" id="_forgotPassword">
            @{ Html.RenderAction("ForgotPassword", "Account");}

        </div>
        <div class="popup-wrapper" style="width:auto; height:auto;"></div>
