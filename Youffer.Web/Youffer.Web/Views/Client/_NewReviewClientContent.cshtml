@model List<Youffer.Web.Resources.ViewModel.WebClientWishListModel>

@if (Model.Any())
{
    foreach (Youffer.Web.Resources.ViewModel.WebClientWishListModel item in Model)
    {
        <tr id="Detail_@(item.WishListId)" data-interest="@item.PurchasedInterest" data-companyid="@item.CompanyId" data-firstname="@item.Client.FirstName" data-imageurl="@(item.Client.ImageUrl !=null ?@item.Client.ImageUrl:@Youffer.Web.Common.Dummy.DummyData.NoImage)" data-age="@item.Client.Age">
            <td title="View client profile" data-wishlistid="@item.WishListId" onclick="GetclientProfileAndReview('@item.Client.ClientId', '@item.PurchasedInterest',true);" style="cursor: pointer;">
                <div class="name">
                    <div>
                        <table>
                            <tr style="border-bottom: 0; height: 30px;">
                                <td style="padding-right: 3px;">
                                    @if (!string.IsNullOrWhiteSpace(item.Client.ImageUrl))
                                    {
                                        <div class="smaller-image" style="background: url(@item.Client.ImageUrl) center no-repeat; background-size: cover;"></div>
                                    }
                                    else
                                    {
                                        <div class="smaller-image" style="background: url(@Youffer.Web.Common.Dummy.DummyData.NoImage) center no-repeat; background-size: cover;"></div>
                                    }
                                </td>
                                <td style="padding-right: 3px;">
                                    <div style="width: 70px; word-break:break-all;">@item.Client.FirstName</div>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>           
            </td>
            <td data-wishlistid="@item.WishListId" onclick="GetclientProfileAndReview('@item.Client.ClientId', '@item.PurchasedInterest',true);" style="cursor: pointer;">@item.CreatedOn.ToString("MM/dd/yyyy")</td>
            <td style="text-align: center;">
                <span data-wishlistid="@item.WishListId" title="Write a review" style="cursor: pointer; color: #1699cd;" id="rate-@(item.Client.ClientId)" onclick="ShowReviewForm(this);">
                    <img src="data:image/png;base64," style="margin-right: 4px;" title="Write a review" />
                </span>
            </td>
            <td data-wishlistid="@item.WishListId" style="text-align: center;">
                <span title="Mark as purchased" id="purchase-@(item.Client.ClientId)" data-wishlistid="@item.WishListId" style="cursor: pointer;" onclick="MarkAsPurchased(this);">
                    <img src="data:image/png;base64," style="margin-right: 4px;" title="Mark as purchased" />
                </span>
            </td>
        </tr>
    }
}

