@model List<Youffer.Web.Resources.ViewModel.WebClientWishListModel>

@if (Model.Any())
{
    foreach (Youffer.Web.Resources.ViewModel.WebClientWishListModel item in Model)
    {
        <tr id="Detail_@(item.WishListId)" data-interest="@item.PurchasedInterest" data-companyid="@item.CompanyId" data-firstname="@item.Client.FirstName" data-imageurl="@(item.Client.ImageUrl !=null ?@item.Client.ImageUrl:@Youffer.Web.Common.Dummy.DummyData.NoImage)" data-age="@item.Client.Age">
            <td onclick="GetclientProfileAndReview('@item.Client.ClientId', '@item.PurchasedInterest',true);" style="cursor: pointer;">
                <div class="name">
                    <div style="min-width:100px; text-overflow:ellipsis;">
                        <table>
                            <tr style="border-bottom: 0; height: 30px;">
                                <td style="padding-right: 3px;">
                                    @if (!string.IsNullOrWhiteSpace(item.Client.ImageUrl))
                                    {
                                        <div class="smaller-image" style="background: url(@item.Client.ImageUrl) center no-repeat; background-size: cover;"></div>
                                    }
                                    else
                                    {
                                        <div class="smaller-image" style="background: url(@Youffer.Web.Common.Dummy.DummyData.NoImage) center no-repeat; background-size: cover;"></div>
                                    }
                                </td>
                                <td style="padding-right: 3px;">
                                    <div style="width: 70px; word-break:break-all;">@item.Client.FirstName</div>
                                </td>
                                <td style="padding-right: 3px;">
                                    <div>
                                        @if (item.Client.IsOnline)
                                        {
                                            <img src="data:image/png;base64," title="Online" />
                                        }
                                        else
                                        {
                                            <img src="data:image/png;base64," title="Offline" />
                                        }

                                    </div>
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>          
            </td>
            <td onclick="GetclientProfileAndReview('@item.Client.ClientId', '@item.PurchasedInterest',true);" style="cursor: pointer;">@item.PurchasedInterest</td>
            <td onclick="GetclientProfileAndReview('@item.Client.ClientId', '@item.PurchasedInterest',true);" style="cursor: pointer;">@item.CreatedOn.ToString("MM/dd/yyyy")</td>
            <td style="text-align:center;">
                <span title="Write a review" style="cursor: pointer; color: #1699cd;" id="rate-@(item.Client.ClientId)" data-clientid="@item.Client.ClientId" data-wishlistid="@item.WishListId" data-companyid="@item.CompanyId" onclick="ShowReviewForm(this);">
                    <img src="data:image/png;base64," class="rate-pop" title="Write a Review" />
                </span>
            </td>
            <td style="text-align:center;">
                <span title="Mark as purchased" style="cursor: pointer;" id="purchase-@(item.Client.ClientId)" data-clientid="@item.Client.ClientId" data-wishlistid="@item.WishListId" data-companyid="@item.CompanyId" onclick="MarkAsPurchased(this);">
                    <img src="data:image/png;base64," title="Mark as purchased" />
                </span>
            </td>
        </tr>
    }
}
