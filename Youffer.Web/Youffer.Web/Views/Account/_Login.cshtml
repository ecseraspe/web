@model Youffer.Web.Resources.ViewModel.LoginModel
<script>
    $.validator.unobtrusive.parse(document);
    $(document).ready(function () {
        $("#loginForm input").on("keyup", function () { $("#divLoginFail").html(""); });
    });
</script>
<div class="popup-login">
    <div class="popup-signup" id="popup" style="">
        <div onclick="closeLoginPopup();" class="Cs_close_icon" style="margin-right: 0px"></div>
        <h4 class="text-center">Login</h4>
        <div class="popup-content">
            <!-- login form begin -->
            <div class="popup-content-login" style="display: block;">
                @using (Ajax.BeginForm("Login", "Account", new { }, new AjaxOptions { HttpMethod = "POST", OnBegin = "DisableLogin", OnComplete = "EnableLogin", OnSuccess = "FormLoginSuccess" }, new { @class = "login-form", id = "loginForm" }))
                {
                    @Html.AntiForgeryToken()
                    <div id="divLoginFail" style="margin-top:5px; height:5px;"> @Html.ValidationMessage("loginFail")</div>
                    <div class="input-wrap" style="max-height: 600px; margin-top: 30px;">
                        <div class="register-img-Div"> <img src="data:image/png;base64," style="margin-top: 16px;" class="register-img" /></div>
                        @Html.TextBoxFor(x => x.Email, new { id = "register-input", @placeholder = "Email", type = "email", @class = "loginTxt" })
                        @Html.ValidationMessageFor(x => x.Email)

                        <div class="register-img-Div"> <img src="data:image/png;base64," style="margin-top: 14px;" class="register-img" /></div>
                        @Html.PasswordFor(x => x.Password, new { id = "register-input", @placeholder = "Password", @class = "loginTxt" })
                        @Html.ValidationMessageFor(x => x.Password)

                        <div style="margin-top: -19px; font-size: 15px;"><a onclick="ShowForgotPasswordForm();" style="cursor: pointer;">Forgot Password</a></div>

                        <label style="color:#fff; width:100%;margin-top: 10px;">
                            <input type="submit" id="btnLogin" class="btn btn-default btn-lg" value="Login" style="background:#1699cd; width:100%;color:#FFF; height: 46px; outline:0; text-decoration: none; padding:0px !important;" />
                        </label>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
<script>
    function DisableLogin() {
        $("#btnLogin").prop("disabled", "disabled");
        injectOverlay(".popup-login #popup");
        showOverlay(".popup-login #popup");
        setOverlayCustomClass(".popup-login #popup", "border-radius-large");
        setOverlayImageCustomClass(".popup-login #popup", "login-overlay-img-custom-margin");
    }
    function EnableLogin() {
        $("#btnLogin").removeAttr("disabled");
    }
</script>
